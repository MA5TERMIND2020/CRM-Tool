    <link rel="stylesheet" href="/css/home-styles.css" />
    <title>Sign-Up</title>
  </head>
  <body>
    <!--**********  NAVBAR SECTION ***********-->
    <nav class="navbar navbar-expand-lg primary-color navbar-dark fixed-top">
      <div class="container-fluid">
        <a class="navbar-brand" href="#">CRM Genie</a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarNav"
          aria-controls="navbarNav"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav ms-auto mb-2 mb-lg-0 h6">
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/login">Login/Subscribe</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/register">Sign-Up</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#">About Us</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#">Sign out</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>

<!--**********  MAIN SECTION ***********-->
<!-- BREADCRUMBS -->
<div class="">
  <nav style="--bs-breadcrumb-divider: '>';" aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/dashboard">Dashboard</a></li>
    <li class="breadcrumb-item active" aria-current="page">Purchases</li>
  </ol>
</nav>
</div>

<!-- MAIN -->
<div class="white rounded p-3 d-flex">
  <div class="">


  <h1>Supplier Details</h1>
  <li><%= supplier.name %></li>
  <li><%= supplier.tel %></li>
  </div>
<div class="">
  <h1>List of Products</h1>
  <% for (let product of supplier.products) { %>
    <li><%= product.name %></li>
  <% } %>


</div>

  </div>

<!--**********  FOOTER SECTION *********  -->
<footer class="primary-color text-center padding">
  <div class="d-flex justify-content-between">
    <p>All Rights Reserved. &copy; Stephen Hammonds</p>
    <div class="d-flex justify-content-center align-items-center">
      <a href="#" class="px-3">
        <i class="fab fa-facebook-f"></i>
      </a>
      <a href="#" class="px-3">
        <i class="fab fa-twitter"></i>
      </a>
      <a href="#" class="px-3">
        <i class="fab fa-linkedin-in"></i>
      </a>
      <a href="#" class="px-3">
        <i class="fab fa-instagram"></i>
      </a>
    </div>
  </div>

</footer>
<script src="/testing-scripts.js"></script>
  </body>
</html>
