<%- layout('layout/dashboard-template')%>
<h1 class="mb-3">Add a Supplier</h1>
<form class="reg-form p-3 border" action="/dashboard/suppliers" method="POST">
  <div class="mb-2">
  <label for="name" class="form-label">Supplier Name</label>
  <input type="text" class="form-control" name="name" id="name" placeholder="Supplier name">
  </div>

  <div class="mb-2">
  <label for="tel" class="form-label">Phone Number</label>
  <input type="tel" class="form-control" name="tel" id="tel">
  </div>

  <div class="mb-2">
  <label for="email" class="form-label">Email</label>
  <input type="email" class="form-control" name="email" id="email">
  </div>

  <div class="mb-5">
  <label for="address" class="form-label">Address</label>
  <input type="text" class="form-control" name="address" id="address" placeholder="Address">
  </div>

  <button class="btn btn-primary btn-sm">Submit</button>
</form>
