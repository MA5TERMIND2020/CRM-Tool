<%- layout('layout/dashboard-template')%>
<div class="">
  <nav style="--bs-breadcrumb-divider: '>';" aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/dashboard">Dashboard</a></li>
    <li class="breadcrumb-item"><a href="/dashboard/suppliers">Suppliers</a></li>
    <li class="breadcrumb-item active" aria-current="page">New</li>
  </ol>
</nav>
</div>
<div class="">

<div class="white rounded p-3">
<h1 class="mb-3">Add a Supplier</h1>
<div class="d-flex justify-content-center">
<form class="reg-form border" action="/dashboard/suppliers" method="POST">
  <div class="mb-3">
  <label for="name" class="form-label">Supplier Name</label>
  <input type="text" name="supplier[name]" id="name" placeholder="Supplier name">
  </div>

  <div class="mb-3">
  <label for="tel" class="form-label">Phone Number</label>
  <input type="tel" name="supplier[tel]" id="tel">
  </div>

  <div class="mb-3">
  <label for="email" class="form-label">Email</label>
  <input type="email" name="supplier[email]" id="email">
  </div>

  <div class="mb-5">
  <label for="address" class="form-label">Address</label>
  <input type="text" name="supplier[address]" id="address" placeholder="Address">
  </div>

    <button class="submitBtn btn-gradient py-2">Submit</button>


</form>
</div>
</div>
